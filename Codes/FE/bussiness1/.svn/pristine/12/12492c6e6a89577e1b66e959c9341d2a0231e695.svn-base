/*全文颜色定义*/

/*---------蓝色--------*/
/*蓝色*/
.blue {color: #3898ec;} /*主色调*/
.blue_bg {background-color: #3898ec;}/*主色调*/
.blue_border {border-color: #3898ec;}/*主色调*/

/*深蓝色*/
.blue_comp {color: #2578c0;} /*辅色1*/
.blue_comp_bg {background-color: #2578c0;}
.blue_comp_border {border-color: #2578c0;}

/*浅蓝色*/

.blue_light {color: #82CCF8;} /*辅色2*/
.blue_light_bg {background-color: #82CCF8;}
.blue_light_border {border-color: #82CCF8;}

/*蓝白色*/

.blue_white {color: #D3EDFF;} /*辅色3*/
.blue_white_bg {background-color: #D3EDFF;}
.blue_white_border {border-color: #D3EDFF;}

/*蓝黑色*/
.blue_black {color: #2D4150;}
.blue_black_bg {background-color: #2D4150;}
.blue_black_border {border-color: #2D4150;}

/*---------红色--------*/
.red_rose {color: #DC4437;} /*点缀色-红*/
.red_rose_bg {background-color: #DC4437;}
.red_rose_border {border-color: #DC4437;}

/*---------绿色--------*/
.green {color: #109D59;} /*点缀色-绿*/
.green_bg {background-color: #109D59;}
.green_border {border-color: #109D59;}

/*---------橙色--------*/
.orange { color: #FF8C05;}/*点缀色-橙*/
.orange_bg {background-color: #FF8C05;}
.orange_border {border-color: #FF8C05;}

.orange_red { color: #fa7c32;}/*点缀色-橙红*/
.orange_red_bg {background-color: #fa7c32;}
.orange_red_border {border-color: #fa7c32;}

/*---------白色--------*/
.white {color: #fff;}
.white_bg {background-color: #fff;}
.white_border {border-color: #fff;}

.white_light  {color: #EFF7FA;}/*背景颜色*/
.white_light_bg  {background-color: #EFF7FA;}/*背景颜色*/
.white_light_border  {border-color: #EFF7FA;}/*背景颜色*/

/*登陆注册输入框背景色*/
.white_input {color: #f4f4f4;}
.white_input_bg {background-color: #f4f4f4;}
.white_input_border {border-color: #f4f4f4;}

/*---------黑色--------*/
.black {color: #000;}
.black_bg {background-color: #000;}
.black_border {border-color: #000;}

/*---------灰色--------*/

/*深灰色*/
.grey_deep {color: #717d86;} /*辅色4*/
.grey_deep_bg {background-color: #717d86;}
.grey_deep_border {border-color: #717d86;}

/*灰色*/
.grey {color: #aaa;}
.grey_bg {background-color: #aaa;}
.grey_border {border-color: #aaa;}

/*灰蓝深色*/
.grey_blue_deep {color: #3C4A55;}
.grey_blue_deep_bg {background-color: #3C4A55;}
.grey_blue_deep_border {border-color: #3C4A55;}

/*灰蓝*/
.grey_blue {color: #63717B;}
.grey_blue_bg {background-color: #63717B;}
.grey_blue_border {border-color: #63717B;}

/*浅灰蓝色*/
.grey_blue_light {color: #728492;}
.grey_blue_light_bg {background-color: #728492;}
.grey_blue_light_border {border-color: #728492;}

/*浅灰色*/
.grey_light {color: #9EABB8;}
.grey_light_bg {background-color: #9EABB8;}
.grey_light_border {border-color: #9EABB8;}

/*灰褐色--按钮不可用背景色*/
.dust_color {color: #9b9b9b;}
.dust_color_bg {background-color: #9b9b9b;}
.dust_color_border {border-color: #9b9b9b;}


/*---------线条边框颜色--------*/
.gray_deep_border {border-color: #C4CED8;}
.gray_light_border {border-color: #DEE4E9;}
