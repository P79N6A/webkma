<div style="width:100%;height:100%;">
    <div class="vote {{{data.selectStyle}}}" data-afterbtn="{{{data.voteAfterBtnStr}}}"  data-controlId="{{{data.controlId}}}">
       
        {{{each data.settingData.itemList as row index}}}
        {{{if data.selectStyle == 'style1'}}}
        <div class="item" style="width:{{{if data.settingData.itemList.length>1}}} 50% {{{else if data.settingData.itemList.length<=1}}} 100% {{{/if}}};">
            <div style="margin-bottom:0.3125rem;">
                <img class="item-image" src="{{{row.image}}}" alt="图片" style="height:{{{if data.settingData.itemList.length>1}}} 4.375rem {{{else if data.settingData.itemList.length<=1}}} auto {{{/if}}};" />
            </div>
            <div style="display:flex;flex-direction: column;text-align: center;justify-content:center;min-height:1.21rem;overflow:hidden;">
                <p class="item-name" style="color: {{{row.titleColor}}}">
                    {{{row.title}}}
                </p>
            </div>
            <p class="vote-num" style="color: {{{row.numColor}}}" data-index="{{{index}}}">{{{row.num}}}</p>
            <div class="submit" style="color: {{{data.voteBeforeBtn.color}}};background-color: {{{data.voteBeforeBtn.bg}}}" data-index="{{{index}}}" data-item="{{{row.title}}}">
                {{{data.voteBeforeBtn.text}}}
            </div>
        </div>
        {{{else if data.selectStyle == 'style2'}}}
        <div class="item" style="width:100%;">
            <div class="image-wrap">
                <img class="item-image" src="{{{row.image}}}" alt="图片" />
            </div>
            <div class="desc">
                <div style="display:flex;flex-direction: column;justify-content:center;min-height:1.21rem;overflow:hidden;">
                    <p class="item-name" style="color: {{{row.titleColor}}}">
                        {{{row.title}}}
                    </p>
                </div>
                <div style="margin-top: 0.625rem;overflow: hidden;"> 
                    <span class="pull-left vote-num" style="color: {{{row.numColor}}}" data-index="{{{index}}}">{{{row.num}}}</span>
                    <div class="submit" style="color: {{{data.voteBeforeBtn.color}}};background-color: {{{data.voteBeforeBtn.bg}}}" data-index="{{{index}}}" data-item="{{{row.title}}}">
                        {{{data.voteBeforeBtn.text}}}
                    </div>
                </div>
            </div>
        </div>
        {{{else if data.selectStyle == 'style3'}}}
        <div class="item" style="width:100%;">
            <p class="item-name1" style="color: {{{row.titleColor}}}">{{{row.title}}}</p>
            <span class="vote-num" style="color: {{{row.numColor}}}" data-index="{{{index}}}">{{{row.num}}}</span>
            <div class="submit" style="color: {{{data.voteBeforeBtn.color}}};background-color: {{{data.voteBeforeBtn.bg}}}" data-index="{{{index}}}" data-item="{{{row.title}}}">
                {{{data.voteBeforeBtn.text}}}
            </div>
        </div>    
        {{{else if data.selectStyle == 'style4'}}}
        <div class="item" style="width:100%;">
            <p class="vote-num" style="color: {{{row.numColor}}}" data-index="{{{index}}}">{{{row.num}}}</p>
            <div class="submit" style="color: {{{data.voteBeforeBtn.color}}};background-color: {{{data.voteBeforeBtn.bg}}}" data-index="{{{index}}}" data-item="{{{row.title}}}">
                {{{data.voteBeforeBtn.text}}}
            </div>
        </div>
        {{{/if}}}
        {{{/each}}}
            
    </div>
</div>