#_panelId_ .game-containner{
    width: 100%;
    height: 100%;
    box-sizing: border-box;
    position: relative;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
}
#_panelId_ .frame-container{
    width: 100%;
    height: 100%;
    border: none;
    box-sizing: border-box;
    /* opacity: 0;
    -webkit-transition: opacity ease-in 1s;
    transition: opacity ease-in 1s; */
}
#_panelId_ .cover{
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    width: 100%;
    height: 100%;
    background-color: transparent;
    background-size: cover;
    background-repeat: no-repeat;
    z-index: 9;
    background-position: center;
}
#_panelId_ .feature-layer{
    background-color: transparent;
    width: 60px;
    height: auto;
    position: absolute;
    right: 15px;
    top: 30px;
    z-index: 9;
}
#_panelId_ .feature-item{
    width: 100%;
    width: 0.9rem;
    height: 0.9rem;
    margin-bottom: 10px;
    cursor: pointer;
    background: no-repeat center;
    background-size: cover;
}
#_panelId_ .game-quit{
    background-image: url(https://resource.ikmark.com/tuixiaobao/weapp/game_quit.png);
    display: none;
}