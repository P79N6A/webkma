
#_panelId_ {
    font-size: 0.375rem;
}
    #_panelId_ .error{
        border-color: #ff685d !important;
    }
    #_panelId_ .input-wrap{
        height: 1.0625rem;
    }
    #_panelId_ .mb5 {
        margin-bottom: 0.1875rem;
    }
    #_panelId_ .pull-left {
        float: left;
    }
    #_panelId_ .clearfix {
        clear: both;
    }
    #_panelId_ .text-center{
        text-align: center;
    }
    #_panelId_ .need_tag {
        float: left;
        color: #ff685d;
        font-size: 0.4rem;
        margin: 0.24rem 0.08rem 0 0;
        line-height: 1.0215;
    }
    input, button, select, textarea, a, img {
        outline: none;
        text-decoration: none;
    }
    #_panelId_ .item-title {
        position: relative;
        width: 100%;
        height: auto;
        line-height: 0.8125rem;
        font-size: 0.375rem;
        padding-left: 0.3125rem;
        padding-top: 0.1rem;
        margin-top: -0.5rem;
    }
    #_panelId_ .err_msg {
        color: #ff685d;
        margin-left: 0.1875rem;
    }
    #_panelId_ .form-control {
        display: block;
        width: 100%;
        height: 1.0625rem;
        padding: 0.1875rem 0.375rem;
        font-size: 0.4375rem;
        line-height: 1.42857143;
        color: #555;
        background-color: #fff;
        background-image: none;
        border: 1px solid #ccc;
        border-radius: 0.125rem;
        -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075);
        box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075);
        -webkit-transition: border-color ease-in-out .15s, -webkit-box-shadow ease-in-out .15s;
        transition: border-color ease-in-out .15s, -webkit-box-shadow ease-in-out .15s;
        transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;
        transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s, -webkit-box-shadow ease-in-out .15s;
    }
    #_panelId_ .radio label, #_panelId_ .checkbox label {
        min-height: 0.625rem;
        padding-left: 0.3125rem;
        margin-bottom: 0;
        font-weight: normal;
        cursor: pointer;
    }
    #_panelId_ textarea.form-control {
        height: auto;
    }
    #_panelId_ .btn {
        display: inline-block;
        padding: 0.1875rem 0.375rem;
        margin-bottom: 0;
        font-size: 0.4375rem;
        font-weight: normal;
        line-height: 1.42857143;
        text-align: center;
        white-space: nowrap;
        vertical-align: middle;
        -ms-touch-action: manipulation;
        touch-action: manipulation;
        cursor: pointer;
        -webkit-user-select: none;
        -moz-user-select: none;
        -ms-user-select: none;
        user-select: none;
        background-image: none;
        border: 1px solid transparent;
        border-radius: 0.125rem;
    }
    #_panelId_ .radio,#_panelId_ .checkbox {
        position: relative;
        display: block;
        margin-top: 0.3125rem;
        margin-bottom: 0.3125rem;
    }

        #_panelId_ .image_group .image_option {
            font-weight: normal;
            display: inline-block;
        }

            #_panelId_ .image_group .image_option .option_img {
                width: 90%;
            }

    #_panelId_ > div, #_panelId_ > div > form {
        width: 100%;
        height: 100%;
    }

    #_panelId_ .btn-submit {
        width: 100%;
    }

    #_panelId_ .checkbox,
    #_panelId_ .radio {
        cursor: pointer;
    }

    #_panelId_ .iconfont {
        font-size: 0.5rem;
        color: #c4ced8;
        vertical-align: middle;
        margin-right: 0.15625rem;
    }
    #_panelId_ .icon-radio-checkded,#_panelId_ .icon-checkbox-checked {
        color: #3899ec;
    }

    #_panelId_ .upload-wrap {
        width: 2.66rem;
        height: 2.66rem;
        position: relative;
        border: 1px dotted #dfe5ec;
        margin-right: 0.1578rem;
        float: left;
      }
      #_panelId_ .upload-wrap input{
        position: absolute;
        top: 0;
        left: 0;
        display: block;
        width: 100%;
        height: 100%;
        opacity: 0;
        filter:alpha(opacity=0);/* 兼容IE */
        z-index: 2;  
      }
      #_panelId_ .upload-wrap .icon-plus{
        position: absolute;
        top: 0px;
        left: 0px;
        font-size: 0.6rem;
        color: #8c939d;
        width: 2.66rem;
        height: 2.66rem;
        line-height: 2.66rem;
        text-align: center;
        z-index: 1;
      }
      #_panelId_ .upload-wrap .avatar {
        position: absolute;
        top: 0px;
        left: 0px;
        width: 2.585rem;
        height: 2.585rem;
        display: block;
        z-index: 1;
      }
      #_panelId_ .upload-wrap .el-upload__tip{
        position: absolute;
        width: 100%;
        height: 0.836rem;
        left: 0;
        bottom: 0;
        padding: 0.15rem 0 0;
        font-size: 0.375rem;
        background-color: rgba(0, 0, 0, 0.3);
        color: #fff;
        z-index: 3;
      }