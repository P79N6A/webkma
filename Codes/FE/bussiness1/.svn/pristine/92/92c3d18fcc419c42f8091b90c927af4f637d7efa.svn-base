    right: 0;
}

.design .ui-widget-content .module_border.bottom {
    width: 100%;
    border-bottom: 1px solid #3899ec;
    bottom: 0;
}

.design .ui-widget-content .module_border.left {
    height: 100%;
    border-left: 1px solid #3899ec;
}

