/*选中时样式*/
* {
    outline: 0px none;
}

/* Header Begin*/
.design .header {
    background: #fff;
    position: fixed;
    top: 0;
    left: 0px;
    z-index: 10091; /*模块边框滚动到头部层级高于头部了  所以从10088修改为10091*   mars  v1.6.8/
    width: 100%;
    height: 50px;
    /*line-height: 50px;*/
    min-width: 1200px;
    padding: 0px;
    background-image: linear-gradient(to right, #02c1cc, #5d5fac);
    /*-moz-box-shadow: 0px 2px 2px #999;
    -webkit-box-shadow: 0px 2px 2px #999;
    box-shadow: 0 0 9px 2px #999;*/
}

   .design .header .logo {
        background: url('/static/images/logo150x50.png') no-repeat center;
         background-size: 120px 40px; 
        text-align: center;
        margin:5px 0 0 60px;
        width: 120px;
        height: 40px;
    }

    .header .head_middle {
        position: absolute;
        left: 0;
        width: 100%;
    }

    .header .middle {
        width: 330px;
        margin: 0 auto;
        text-align: center;
    }

        .header .middle .login_status a {
            cursor: pointer;
            text-decoration: underline;
            margin: 0px 3px;
        }

        .header .middle .templateName {
            width: 330px;
        }

            .header .middle .templateName .nameInput {
                margin-top: 8px;
            }

            .header .middle .templateName:hover {
                background-color: #d3edfe;
            }

    .header .features {
        list-style: none;
        padding: 0px;
        margin: 0px;
    }

        .header .features li {
            float: left;
            width: 140px;
            height: 34px;
            line-height: 34px;
            color: #fff;
            text-align: center;
            background: #24d0c6;
            margin-right: 10px;
            margin-top: 8px;
            cursor: pointer;
        }

.header .head_btn  i{
    margin-right: 3px;
    font-size: 12px !important;
}


.design {
    font-size: 12px;
}

/* Left Menu Begin*/
