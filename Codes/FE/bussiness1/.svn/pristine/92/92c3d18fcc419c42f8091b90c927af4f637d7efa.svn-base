            color: white;
            font-weight: bold;
            box-shadow: 0px 0px 20px rgba(0, 0, 0, 0.24);
        }

        .design .pageeditor-mobile #content #container .phone-body {
            width: 100%;
            height: auto;
            box-shadow: 0px 0px 20px rgba(0, 0, 0, 0.24);
        }

            .design .pageeditor-mobile #content #container .phone-body .table-panel {
                width: 320px;
                margin: 0px auto;
                height: 100%;
                position: relative;
            }

                .design .pageeditor-mobile #content #container .phone-body .table-panel .page-tpl-header {
                    top: 0;
                    width: 100%;
                    height: 40px;
                    position: relative;
                    box-shadow: 0 0 2px 0 #bbb;
                    /*z-index: 0;*/
                }

                    .design .pageeditor-mobile #content #container .phone-body .table-panel .page-tpl-header .header-panel {
                        height: 38px;
                        width: 100%;
                        background-color: #ffffff;
                    }

                        .design .pageeditor-mobile #content #container .phone-body .table-panel .page-tpl-header .header-panel .custom-panel {
                            height: 100%;
                            width: 25%;
                            float: right;
                        }

                .design .pageeditor-mobile #content #container .phone-body .table-panel .page-tpl-body {
                    width: 100%;
                    position: relative;
                }

                    .design .pageeditor-mobile #content #container .phone-body .table-panel .page-tpl-body .edit-panel {
                        height: 100%;
                    }

                .design .pageeditor-mobile #content #container .phone-body .table-panel .page-tpl-footer {
                    bottom: 0;
                    width: 100%;
                    height: 40px;
                    position: relative;
                    box-shadow: 0 -1px 2px 0 #bbb;
                    /*z-index: 0;*/
                }

                    .design .pageeditor-mobile #content #container .phone-body .table-panel .page-tpl-footer .footer-panel {
                        height: 100%;
                    }

    .design .pageeditor-mobile #content #content-wrap .phone-footer {
        width: 320px;
        position: relative;
        height: 56px !important;
        text-align: center;
        overflow: hidden;
        background-color: #fff;
        margin: auto;
        border-bottom-left-radius: 20px;
        border-bottom-right-radius: 20px;
        box-shadow: 0px 0px 20px rgba(0, 0, 0, 0.24);
    }

        .design .pageeditor-mobile #content #content-wrap .phone-footer span {
            font-size: 16px;
        }

        .design .pageeditor-mobile #content #content-wrap .phone-footer #resizeHandle {
            width: 100%;
            height: 20px;
            margin-top: 14px;
            text-align: center;
        }

.pageeditor-mobile .phone-footer #resizeHandle .heightDown,
.pageeditor-mobile .phone-footer #resizeHandle .heightUp {
    display: inline-block;
    width: 18px;
    height: 16px;
    vertical-align: baseline;
