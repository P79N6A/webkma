/* PC Right Editor START*/
.design .pageeditor #content {
    /*background-color: #f2f2f2;*/
    position: relative;
    /*margin-top: 50px;*/
    width: auto;
}

    .design .pageeditor #content #container > .content-body {
        width: 100%;
        margin: 0 auto;
        height: auto;
    }

        .design .pageeditor #content #container > .content-body .content-border-left {
            float: left;
            height: 100%;
            border-left: 1px dashed #999;
        }

        .design .pageeditor #content #container > .content-body .content-border-right {
            float: right;
            height: 100%;
            border-right: 1px dashed #999;
        }

        .design .pageeditor #content #container > .content-body > .content-body-body {
            height: 100%;
            margin: auto;
            position: relative;
            /*border-bottom: 1px dashed #999;*/
        }
.design #content-resize-box  .ui-resizable-handle.ui-resizable-s:before {
    content: " ";
    background-color: #fff; /* ie 下绝对定位原始一定要设置背景色或border使其占据位置 */
    opacity: 0;
    display: block;
    height: 30px;
    text-align: center;
    color: #2d3d34;
    border-radius: 5px 5px 0 0;
    margin: 0 auto 0px auto;
    font-size: 16px;
    width: 190px;
    position: absolute;
    left: 0;
    right: 0;
    top: 13px;
    z-index: 99;
}

.design .pageeditor #content .content-footer {
    position: absolute;
    bottom: 0;
    left: 50%;
    width: 1200px;
    margin: 0 0 -40px -600px;
    height: 40px;
    line-height: 40px;
    font-size: 16px;
    text-align: center;
    background-color: #fff;
    border: 1px dashed #999;
    z-index: 1;
}

.design .pageeditor #content #resizeHandle {
    width: 100%;
    height: 20px;
    text-align: center;
}

    .design .pageeditor #content #resizeHandle .heightDown,
    .design .pageeditor #content #resizeHandle .heightUp {
        display: inline-block;
        width: 18px;
        height: 16px;
        vertical-align: baseline;
        margin-right: 10px;
        margin-left: 10px;
        background: url(../assets/images/design/arrow_down.png) no-repeat;
    }

        .design .pageeditor #content #resizeHandle .heightDown:hover {
            background: url(../assets/images/design/arrow_down_hover.png) no-repeat;
        }

    .design .pageeditor #content #resizeHandle .heightUp {
        background: url(../assets/images/design/arrow_up.png) no-repeat;
    }

        .design .pageeditor #content #resizeHandle .heightUp:hover {
            background: url(../assets/images/design/arrow_up_hover.png) no-repeat;
        }

/* PC Right Editor END*/

/* Mobile Right Editor START*/
.design .pageeditor-mobile #content {
    position: relative;
    margin: 50px auto 0 0px;
    padding-top: 0px;
    /*z-index: 0;*/
    width: auto;
    background-size: 100% 100%;
}

    .design .pageeditor-mobile #content #container {
        width: 320px;
        height: 100%;
        /* right: 60px; */
        margin: 0 auto;
        padding-top: 50px;
        position: relative;
        /*padding-bottom: 50px;*/
    }
    .design .pageeditor-mobile #content-resize-box{
        height:auto;
    }
    
        .design .pageeditor-mobile #content #container > * {
            background-color: white;
        }

        .design .pageeditor-mobile #content #container .phone-header {
            border-top-left-radius: 20px;
            border-top-right-radius: 20px;
