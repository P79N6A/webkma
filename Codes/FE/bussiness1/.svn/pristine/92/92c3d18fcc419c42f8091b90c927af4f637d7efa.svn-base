.design-selector{
    height: 35px;
    width: 165px;
    display: inline-block;
    border: 1px solid #c9d4df;
    position: relative;
    border-radius: 3px;
}
.design-selector .design-selector-value{
    height: 100%;
    padding: 0 35px 0 15px;
    overflow: hidden;
    line-height: 32px;
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;
}
.design-selector select{
    border: none;
    width: 100%;
    height: 100% !important;
    background: transparent;
    position: absolute;
    top: 0;
    opacity: 0;
    z-index: 1;
}
.design-selector .design-selector-value:before{
    content: '';
    width: 35px;
    height: 35px;
    display: block;
    background-color: #00bad0;
    position: absolute;
    top: -1px;
    right: 0;
    border-bottom-right-radius: 3px;
    border-top-right-radius: 3px;
}
.design-selector .design-selector-value:after{
    content: '';
    display: block;
    border: 7px solid #fff;
    border-bottom: none;
    border-left-color: transparent;
    border-right-color: transparent;
    margin: auto;
    position: absolute;
    top: 14px;
    right: 11px;
    width: 0px;
}
