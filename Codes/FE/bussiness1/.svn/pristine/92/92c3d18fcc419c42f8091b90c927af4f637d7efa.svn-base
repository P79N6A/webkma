/* jQuery UI repair */
.design .pageeditor .ui-widget-content,
.design .pageeditor-mobile .ui-widget-content {
    /*border: 1px solid transparent;*/
    border: 0px none;
}

    /* .design .pageeditor .ui-widget-content:not(.on):hover > span.module-top-tip,
    .design .pageeditor-mobile .ui-widget-content:not(.on):hover > span.module-top-tip {
        display: block;
    } */

    .design .pageeditor .ui-widget-content .module_border,
    .design .pageeditor-mobile .ui-widget-content .module_border {
        position: absolute;
        z-index: 10089;
        display: none;
    }

    .design .pageeditor .ui-widget-content.hover .module_border,
    .design .pageeditor-mobile .ui-widget-content.hover .module_border,
    .design .pageeditor .ui-widget-content.new .module_border,
    .design .pageeditor-mobile .ui-widget-content.new .module_border,
    .design .pageeditor .ui-widget-content.hover-target .module_border,
    .design .pageeditor-mobile .ui-widget-content.hover-target .module_border,
    .design .pageeditor .ui-widget-content.on .module_border,
    .design .pageeditor-mobile .ui-widget-content.on .module_border {
        display: block;
        /*border: 1px solid #3899ec;*/
    }

.design .ui-widget-content .module_border.top {
    width: 100%;
    border-top: 1px solid #3899ec;
}

.design .ui-widget-content .module_border.right {
    height: 100%;
