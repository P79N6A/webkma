* {
    box-sizing: border-box;
}

html,
body {
    overflow: hidden;
}

.page-body {
    position: relative;
    overflow: hidden;
    margin: 0px auto;
    width: 10rem;
}

.page-body .page-panel {
    width: 100%;
    overflow: hidden;
    position: relative;
    background-color: white;
    -webkit-box-shadow: 0px 0px 20px rgba(0, 0, 0, 0.24);
    box-shadow: 0px 0px 20px rgba(245, 102, 102, 0.24);
    opacity: 0;
    /* transition: opacity 2s;
    -moz-transition: opacity 2s;
    -webkit-transition: opacity 2s;
    -o-transition: opacity 2s; */
    display: none;
}

.page-body .page-panel .page-content {
    width: 100%;
    height: 100%;
    position: relative;
}

.page-body .page-show {
    opacity: 1;

}

.layer-hiden {
    transition: opacity 2s;
    opacity: 0;
}

.page-qrcode {
    background: #fff;
    width: 100%;
    height: 100%;
    text-align: center;
    color: #9eabb8;
    padding: 200px 20px 0;
    display: none;
}

.page-qrcode [data-qrcode] {
    width: 200px;
    height: 100%;
    margin: 0 auto 20px;
}

.swiper-container {
    height: 100%;
    width: 100%;
    /* display: flex;
    align-items: center; */
}

.swiper-slide img {
    width: 100%;
    height: 100%;
}

.page-body .page-panel,
.page-body .layui-m-layer {
    animation-duration: 1s;

}

.page-body .ani {
    visibility: hidden;
}

@keyframes tipmove {
    0% {
        bottom: 5px;
        opacity: 0
    }
    50% {
        bottom: 15px;
        opacity: 1
    }
    100% {
        bottom: 25px;
        opacity: 0
    }
}

@-webkit-keyframes tipmove {
    0% {
        bottom: 5px;
        opacity: 0
    }
    50% {
        bottom: 15px;
        opacity: 1
    }
    100% {
        bottom: 25px;
        opacity: 0
    }
}

.page-body .arrow {
    position: fixed;
    bottom: 0;
    top: auto;
    left: 0;
    right: 0;
    width: 1.5625rem;
    height: 1.5625rem;
    margin: auto;
    background: transparent;
    cursor: pointer;
    /*border: none;
    border-top: 4px solid #fff;
    border-left: 4px solid #fff;
    box-sizing: border-box;
    transform: rotateZ(-45deg);
    -webkit-transform: rotateZ(45deg);
    animation: tipmove 1.5s infinite ease-in-out; 
    -webkit-animation: tipmove 1.5s infinite ease-in-out;*/
    z-index: 99999;
}

.page-body .arrow:before {
    content: '';
    position: absolute;
    display: block;
    bottom: 0;
    top: auto;
    left: 0;
    right: 0;
    width: 0.625rem;
    height: 0.625rem;
    margin: auto;
    border: none;
    border-top: 0.125rem solid #fff;
    border-left: 0.125rem solid #fff;
    box-sizing: border-box;
    transform: rotateZ(-45deg);
    -webkit-transform: rotateZ(45deg);
    animation: tipmove 2s infinite ease-in-out;
    -webkit-animation: tipmove 2s infinite ease-in-out;
    border-color: #000;
}
.swiper-scrollbar{
    background: rgba(241, 243, 252,.1);
    opacity: 0; 
    width: 2px !important;
    right: 0 !important;
    transition-duration: 600ms;
    -webkit-transition-duration:600ms; 
}
.swiper-scrollbar-drag {
    background: rgba(99, 99, 99, 0.5);
}

.popup-btn {
    position: absolute;
    width: 1rem;
    height: 1rem;
}

.popup-btn .btn-info {
    width: 100%;
    height: 100%;
    background-repeat: no-repeat;
    background-size: 100%;
    border-radius: 50%;
}

.popup-btn .btn-info.icon-activity {
    background-image: url('/public/images/yes.png');
}

.popup-btn .btn-info.icon-prize {
    background-image: url('/public/images/btn_up.png');
}

.popup-btn .btn-info.icon-qrcode {
    background-image: url('/public/images/close2.png');
} 

/* 文字居左对齐 */
.text-left {
    text-align: left;
}
/* 超小号 */
.mini-font {
    font-size: 0.293rem !important;
}
/* 小号文字 */
.small-font {
    font-size: 0.346rem !important;
}
/* 中号文字 */
.medium-font {
    font-size: 0.4rem !important;
}
/* 大号文字 */
.big-font {
    font-size: 0.48rem !important;
}

/* 重写滚动条样式 */
.scroll-box::-webkit-scrollbar-track-piece {
    background-color: rgba(0, 0, 0, 0);
    border-left: 0.1333rem solid rgba(0, 0, 0, 0);
}

.scroll-box::-webkit-scrollbar {
    width: 0.08rem;
    height: 0.174rem;
    -webkit-border-radius: 0.666rem;
    -moz-border-radius: 0.666rem;
    border-radius: 0.666rem;
}

.scroll-box::-webkit-scrollbar-thumb {
    background-color: rgba(0, 0, 0, 0.3);
    background-clip: padding-box;
    -webkit-border-radius: 0.666rem;
    -moz-border-radius: 0.666rem;
    border-radius: 0.666rem;
    min-height: 0.374rem;
}

.scroll-box::-webkit-scrollbar-thumb:hover {
    background-color: rgba(0, 0, 0, 0.3);
    -webkit-border-radius: 0.666rem;
    -moz-border-radius: 0.666rem;
    border-radius: 0.666rem;
}
#msg-model {
    display: none;
    position: fixed;
    width: 100%;
    height: 100vh;
    top: 0;
    left: 0;
    z-index: 29891000;
    background-color: transparent;
}
#msg-model .msg-content {
    width: 100%;
    position: absolute;
    top: 48vh;
    left: 0;
    text-align: center;
}
#msg-model span {
    display: inline-block;
    padding: 0.16rem 0.4rem;
    min-width: 1.86rem;
    background-color: rgba(0, 0, 0, 0.6);
    color: #fff;
    border-radius: 0.12rem;
}
