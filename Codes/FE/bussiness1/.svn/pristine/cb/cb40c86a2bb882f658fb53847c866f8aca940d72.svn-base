/* 快速导航 */
#fastNav{
    position: fixed;
    bottom:1rem;
    right:0.4rem;
    z-index:9999;
}
#fastNav .nav-spread{
    width: 1.33rem;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    align-items: center;
    border-radius: 1.333rem;
    background-color: rgba(0,0,0,.7);
    padding: 0.45rem 0;
    box-sizing: border-box;
    border: 0.0133rem solid rgba(0,0,0,.2);
    box-shadow: 0rem 0.06rem 0.133rem rgba(0,0,0,.2);
}
#fastNav .nav-spread > div{
    width: 100%;
    font-size: 0.293rem;
    color: #fff;
    text-align: center;
    margin-bottom: 0.45rem;
}
#fastNav .nav-spread > div:last-child{
    margin-bottom: 0;
}
#fastNav .nav-spread > div img{
    display: block;
    width: 0.4rem;
    height: 0.4rem;
    margin: 0 auto 0.12rem;
}

/* 推广人员底部功能栏 */
#extensionNav{
    position: fixed;
    bottom: 0;
    width: 100%;
    height: 8vh;
    padding: 1vh 0;
    background: rgba(0,0,0,0.7);
    z-index: 100;
}
#extensionNav .menus-bar{
    width: 100%;
    height: 100%;
}
#extensionNav .menus-bar .column-button{
    float:left; 
    height: 6vh; 
    line-height: 6vh;
    text-align: center; 
    color: #fff; 
    display: flex;
    align-items: center; 
    align-content: center;
    justify-content: center; 
    display: -webkit-flex; 
    -webkit-align-items: center;
    -webkit-align-content: center;
    -webkit-justify-content: center;
}