<!--pages/cash-withdrawal/cash-withdrawal.wxml-->
<view class='cash-withdrawal'>
  <view class='box'>
    <view class='title'>
      提现到微信零钱
    </view>
    <view class='sub-title'>提现金额</view>
    <view>
      <input class="money-input" bindinput="bindInput" placeholder-class="phcolor" value='{{cashMoney}}' />
    </view>
    <view class='des'>
      <text class='des-money'>当前账户余额{{allBalance}}元</text>
      <text class='des-link' bindtap='allCash'>全部提现</text>
    </view>
    <view class="cash-btn">
      <i-button type="success" i-class="cash-btn" disabled="{{!!btnDisabled}}" bind:click="cashWithdrawal">提现</i-button>
    </view>
  </view>
</view>