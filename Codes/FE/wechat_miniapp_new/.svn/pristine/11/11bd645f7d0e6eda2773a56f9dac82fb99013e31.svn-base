/* pages/staff-info/staff-info.wxss */
.container{ 
  height: 100%;
  background-color: #F1F3FC;
}
.margin-top{
  margin-top: 38rpx;
}
.margin-left{
  margin-left: 6rpx;
}
.icon-dott{
  display: inline-block;
  color:#ff5a2a;
  margin-left:-12rpx;
}
.staff-box{
  background-color: #FFFFFF;
}
.staff-box .staff-photo,
.staff-box .staff-info,
.staff-box .staff-phone,
.staff-box .staff-corp,
.privacy-set .title,
.privacy-set .phone,
.privacy-set .wechat,
.privacy-set .email
{
  height: 100rpx;
  font-size: 28rpx;
  line-height: 100rpx;
  color: #000000; 
  padding: 0rpx 30rpx;
  border-top: 2rpx solid #EEEEEE;
  background-color: #fff;
  display: flex;
  justify-content: space-between;
}
.staff-info text:nth-of-type(2){
  text-indent: -125rpx;
}
.staff-photo .staff-Img{
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  margin-top: 10rpx;
} 
.staff-box .staff-info>view,
.staff-box .staff-corp>view,
.staff-box .staff-phone .phone,
.staff-box .staff-phone .untying,
.privacy-set .phone>view,
.privacy-set .wechat>view,
.privacy-set .email>view
{
  font-size: 27rpx;
  color: #999999;
}
.staff-box .staff-info>view{
  width: 400rpx;
  text-align: right;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space:nowrap;  
}
.staff-box .staff-info>input{
  width: 400rpx;
  font-size: 27rpx;
  text-align: right;
  color: #999999;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space:nowrap; 
  margin-top: 25rpx; 
}
.staff-box .staff-phone .phone{
  margin-left: 10rpx;
}
.staff-box .staff-phone .getPhoneNumber{
  width:114rpx;
  height:43rpx;
  font-size:27rpx;
  color:#3898ec;
  line-height:37rpx;
  border-radius:7rpx;
  border:1rpx solid #fff;
  padding:0;
  margin:30rpx 0 0 0;
}
.staff-corp {
  /* border-bottom: 2rpx solid #EEEEEE;  */
  position: relative;
}
.staff-corp .corp-text{
  width: 330rpx;
  text-align:right;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space:nowrap;
  flex-direction: row;
}
.corp-text>input{
  margin:auto 0;
}
.text-color{
  color: #bdbdbd;
}
.switch-arrows{
  width: 25rpx;
  height: 25rpx;
  position: relative;
  top: 3rpx;
}
.bindcolor{
  color: #3898ec;
}
/* 隐私设置 */
.privacy-set{
  height: 500rpx;
  background-color: #F1F3FC;
  margin-bottom: 46rpx;
}
.privacy-set .title{
  font-size: 28rpx;
  color: #999999;
  background-color: #F1F3FC;
}
.privacy-set>view:last-child{
  border-bottom: 1rpx solid #EEEEEE;
}
/* 保存按钮 */
.conserve-btn{
  width: 100%;
  height: 100rpx;
  background-color: #fff;
  border-top: 1rpx solid #EEEEEE;
  margin: 0 auto;
  position: fixed;
  bottom: 0;
}
.conserve-btn button{
  width:93%;
  height:79rpx;
  color:#fff;
  line-height:79rpx;
  border:none;
  border-radius:8rpx;
  background-color:#24D0C6;
  margin:0 auto;
  margin-top:12rpx;
}