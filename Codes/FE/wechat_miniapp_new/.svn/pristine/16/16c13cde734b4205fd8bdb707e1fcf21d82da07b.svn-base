/* pages/extension-task-charts/extension-task-charts.wxss */
@import "/components/icon/iconfont.wxss";
.font-22{
  font-size: 22rpx;
}
.title_top{
  display: flex;
  justify-content: flex-start
}
.title_top view{
  margin-right: 15rpx;
}
.font-18{
  font-size: 18rpx;
}

.num-color{
  color: #F68411;
  margin-left: 5rpx;
}

.border-right{
  border-right: 1rpx solid #DDE4EB;
}

.bd__navbar {
  padding: 0 20rpx;
  background: #fff;
}

.bd__section {
  padding: 0 20rpx;
  background: #fff;
  margin-bottom: 2vh;
}

.bd__section.head__row {
  padding-top: 2vh;
}

.bd__section .head__title {
  padding-top: 1vh;
}

.section_row {
  padding: 3vh 10rpx;
}

.section_row + .section_row {
  border-top: 1rpx solid #f8f8f8;
}

.item-count {
  font-size: 54rpx;
  color: #333333;
}
.plug-list .vote-record{
  height: 90rpx;
  padding: 20rpx 0 0 40rpx;
  background-color: #fff;
}

.bg-title{
  border-top: 1rpx solid #DDE4EB;
  border-bottom: 1rpx solid #DDE4EB;
}

.bg-cont{
  background-color: #fff;
  border-bottom: 1rpx solid #DDE4EB;
}
.plug-list .plug-tab{
  height: 90rpx;
  line-height: 90rpx;
  border-top: 1rpx solid #ccc;
}

.active{
  background-color:#fff;
}

.plug-list .plug-tab > view{
  width: 15%;
  font-size: 26rpx;
  text-align: center;
}
.plug-list .vote-record{
  height: 90rpx;
  padding: 20rpx 0 0 40rpx;
  background-color: #fff;
}
.voteTable{
  margin-top: 20rpx;
}

.voteTable .th {
  height: 90rpx;
  line-height: 90rpx;
  font-size: 35rpx;
}

.voteTable .tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 90rpx;
  align-items: center;
}

.voteTable .td {
  height: 90rpx;
  font-size: 30rpx;
  line-height: 90rpx;
  justify-content: center;
  text-align: center;
}

.voteTable .td:nth-of-type(1){
  width: 20%;
}

.voteTable .td:nth-of-type(2){
  width: 60%;
}

.voteTable .td:nth-of-type(3){
  width: 20%;
}
/* signup_chart */
.signup_chart{
 background: #ffffff;
 padding: 60rpx 40rpx;
 display: flex;
 justify-content: space-between;
 margin-bottom: 25rpx;
}
.signup_left{
  width: 200rpx;
  margin-right: 40rpx;
  font-size: 22rpx;
}
.orange_font{
  color: #F68411 !important;
  font-weight:initial;
}
.price{
  color: #FA766B  !important;
  font-weight:initial;
}
.signup_title{
  color: #3C4A55;
  font-weight: bold
}
.tipcont{
  font-size: 18rpx;
  color: #B1BFCD;
  margin-top: 22rpx;
}
.signup_right{
  width: 420rpx;
  height: 150rpx;
}
.cont_info{
  background: #ffffff;
  padding-top:10rpx; 
}
.cont_info_lsit{
  margin-top: 10px;
  border-top: 1px solid #DDE4EB
}
.search_input{
  width:calc(100vw - 20rpx);
  margin: 0rpx 10rpx;
  height: 70rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  background: #F7F7FF
}
.search_input input{
  font-size: 26rpx;
  width: 635rpx;
}
.search_input text{
  display: block;
  margin-left: 30rpx;
  margin-right: 10rpx;
}

.condition{
  margin-left: 20rpx;
  margin-top: 18px;
  display: flex;
  width:224rpx;
  height:53rpx;
  background:rgba(247,247,255,1);
  opacity:1;
  border-radius:10rpx;
  font-size: 22rpx;
  align-items: center;
  align-self: center;
  justify-content: space-between;
  padding: 0rpx 10rpx;
  overflow: hidden
}
/* 商品 */
.cont_list{
  background: #ffffff;
  border-top: 1px solid #DDE4EB;
}
.list_item{
  display: flex;
  justify-content: flex-start;
  width: calc(100% - 80rpx);
  padding: 30rpx 40rpx;
  border-bottom: 1px solid #DDE4EB
}
.list_item image{
  width: 100rpx;
  height: 100rpx;
  margin-right: 20rpx;
}
.item_info{
  margin-right: 130rpx
}
.item_title{
  font-size: 22rpx;
  width: 310rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.item_price{
  font-size: 20rpx;
  color: #B1BFCD;
}
.item-count text:last-child{
  color: #FA766B !important;
  margin-top: 10rpx;
}
.item_right{
  font-size: 18rpx;
  display: flex;
  flex-direction:column;
  justify-content: space-around;
}
/* 中奖 */
.reward_content{
  margin-top: 20rpx;
  background: #ffffff;
  margin-bottom: 50rpx;
}
.reward_title{
  background: #E5FDFF;
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  border-top: 1px solid #DDE4EB;
  border-bottom:  1px solid #DDE4EB;
  display: flex;
  justify-content: flex-start;
}
.reward_title view{
  text-align: center;
  font-size: 22rpx;
}
.reward_title view:first-child{
  width: 296rpx;
}
.reward_title view:nth-child(2){
  width: 268rpx;
}
.reward_title view:last-child{
  width: 189rpx;
}

.item_list{
  display: flex;
  justify-content: flex-start;
  border-bottom:  1px solid #DDE4EB;
  font-size: 22rpx;
  color: #3C4A55;
  height: 90rpx;
  text-align: center
}
.item_list view{
  border-right: 1px solid #DDE4EB;
  height: 90rpx;
  line-height: 90rpx;
}
.item_list view:last-child{
  border-right:none;
}
.td-1{
  width: 224rpx;
  padding:0px 33rpx;
  background: #E5FDFF;
  overflow: hidden ; 
  white-space: nowrap ; 
  text-overflow: ellipsis ; 
}
.td-2{
  width: 268rpx;
  display: flex;
  justify-content:flex-start;
  align-items: center;
}
.td-2 image{
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
  margin-right: 15rpx;
  margin-left: 21rpx;
}
.td-2 view{
  width: 110rpx;
  overflow: hidden ; 
  white-space: nowrap ; 
  text-overflow: ellipsis ;
}
.td-3{
  width: 189rpx;
  justify-content: center;
  color: #24D0C6;
}
/* mask content */
.mask{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background:rgba(0, 0, 0, 0.7);
  font-size: 22rpx;
  color: #3C4A55;
  z-index: 100
}
.mask_cont{
  background: #ffffff;
  width:600rpx;
  height: calc(100vh - 470rpx);
  border-radius: 10rpx;
  margin: 0 auto;
  margin-top: 20%;
}
.mask_title{
  width: 100%;
  height: 116rpx;
  text-align: center;
  background: #ffffff;
  border-bottom: 1px solid #DDE4EB;
  padding-bottom:10rpx; 
  position: relative;
  border-radius: 10rpx;
}
.mask_title image{
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  position: absolute;
  top:-90rpx;
  margin:0 auto;
  left:0;
  right:0;
}
.mask_name{
  padding-top: 80rpx;
}
.mask-content{
  height: calc(100% - 230rpx); 
  overflow: auto
}
.mask_info_top{
  padding: 0rpx 64rpx;
  padding-top: 150rpx;
  padding-bottom: 44rpx;
}
.maskInfoTop_itme{
  margin-bottom: 11rpx;
  display:flex;
  justify-content: flex-start;
}
.maskInfoTop_itme text:first-child{
  width: 110rpx;
}
.mask_info_top_right{
  width: 350rpx;
  color: #B1BFCD
}
.mask_info_bottom{
  padding: 40rpx 64rpx;
}
.mask_info_item{
  margin-bottom: 20rpx;
  overflow: hidden;
}
.infoFirsttext{
  width: 132rpx;
  float: left
}
.infoLasttext{
  width:340rpx !important;
  height: auto;
  white-space:break-word;
  float: right;
  display: flex;
  flex-direction: column;
}
.last_info_item{
  margin-bottom: 10rpx;
  margin-right: 10rpx;
  display: flex;
  align-items: center
}
.last_info_item image{
  width: 100rpx;
  height: 100rpx;
  margin-right: 10rpx;
}


.mask_info_btns{
  height: 100rpx;
  display: flex;
  justify-content: space-between;
  font-size: 30rpx;
  color: #24D0C6;
  border-top: 1px solid #DDE4EB;
}
.mask_info_btns view{
  display: flex;
  justify-content: center;
  align-items: center;
  width: 300rpx
}
.mask_info_btns view:last-child{
  color: #B1BFCD;
  border-left: 1rpx solid #DDE4EB
}
.icon-guanbi{
  color:#ffffff;
  text-align: center;
  width: 80rpx;
  height: 80rpx;
  margin: 0 auto;
  margin-top:50rpx;
  font-size: 80rpx;
  line-height: 80rpx;
}
/* 任务数据 */
.content_boxs{
  padding: 20rpx 0rpx;
  background: #ffffff;
  margin-bottom: 20rpx;
}
.boxs_cont_info{
  width: calc(100vw - 80rpx);
  margin: 10rpx 40rpx;
}
.heard-title{
  background: #FDF6EC;
  color: #B1BFCD;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 22rpx;
  height: 90rpx;
}
.heard-title text{
  color: #F68411 !important;
  margin: 0rpx 4rpx;
}
.left-title-all{
  width:calc(100vw - 45rpx );
  height: 40rpx;
  padding:0rpx 20rpx;
  border-left: 5rpx solid #24D0C6;
  line-height: 40rpx;
  font-size: 26rpx;
}