@import "../icon/iconfont.wxss";
.content{
  background: #ffffff;
  padding: 30rpx 40rpx;
} 
.list_item{
  display: flex;
  justify-content:flex-start;
  height: 232rpx;
  width:100%;
  align-items: center
}
.item_index{
  font-size: 22rpx;
  color: #B1BFCD;
  margin-right: 40rpx;
}
.left_cont{
  position: relative;
  width: 120rpx;
  height: 230rpx;
  font-size: 22rpx;
}
.item_img{
  width: 100%;
  height: 100%;
}
.left_child{
  position: absolute;
  width: 100%;
  height: 80rpx;
  bottom: 0;
  left: 0;
  right: 0;
  background:rgba(0, 0, 0, 0.7) ;
}
.left_info{
  text-align: center;
  color: #ffffff;
  font-size: 22rpx;
  line-height: 22rpx;
}
.left_info:first-child{
  margin-top: 13rpx;
  margin-bottom: 8rpx;
}
.item_info{
  font-size: 26rpx;
  margin-left: 31rpx; 
  max-width: 326rpx
}
.item_info_no_btn{
  max-width: 500rpx !important
}
.item_info_index{
  margin-right: 66rpx !important;
}
.name{
  width: 100%;
  height: 66rpx;
  line-height: 34rpx;
  margin-bottom: 19rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.name image{
  width: 20rpx;
  height: 21rpx;
  margin-right: 6rpx;
}
.cont_lable{
  display: flex;
  justify-content: flex-start;
  width: 440rpx;
  margin-left: 31rpx; 
}
.cont_lable view{
  padding: 5rpx 10rpx;
  line-height: 32rpx;
  margin-right: 9rpx;
  font-size: 20rpx;
  color: #ffffff;
  text-align: center;
  background: #9EBBFF
}
.cont_lable view:nth-child(1){
   background: #FEDB8C !important
}
.cont_lable view:nth-child(2){
   background: #FFB8DF !important
}
.cont_lable view:nth-child(3){
   background: #8CE2FE !important
}
.cont_lable view:nth-child(4){
   background: #FF9F9F !important
}
.planned_speed{
  display: flex;
  justify-content: flex-start;
}
.mind{
  width: 190rpx;
  margin-top:-3rpx;
}
.cont_bottom{
  margin-bottom: 16rpx;
  color: #B1BFCD;
  font-size: 22rpx;
  line-height: 22rpx;
}
.cont_title{
  margin-right: 15rpx;
}
.orange_font{
  color: #F68411 !important;
  margin-left: 12rpx;
}
.timer_num{
  color: #FA766B!important;
}

.operationBtn{
  width: 80rpx;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-around
}
.operationBtn text{
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 50%;
  width: 80rpx;
  height: 80rpx;
  background: #F7F7FF;
  font-size: 38rpx;
  color: #b1bfcd
}
.icon-company-information{
  font-size: 40rpx !important;
}