ec-canvas {
  width: 100%;
  height: 100%;
}

.cell {
  height: 100%;
  background-color: #fff;
}

.stat-cell {
  height: 100%;
  background-color: #fff;
  font-size: 20rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.row-text {
  text-align: left;
}

.row-icon-total {
  padding-right: 6rpx;
  color: #69e4dd;
}

.row-icon-value {
  padding-right: 6rpx;
  color: #ffcf7c;
}

.row-value {
  padding-left: 6rpx;
  color: #fa766b;
  font-weight: 500;
}
.font-gray{
  color: #b1bfcd !important;
  font-size:22rpx; 
}
